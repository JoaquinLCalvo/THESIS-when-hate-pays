    {
      "cell_type": "code",
      "source": [
        "\n",
        "library(lme4)\n",
        "\n",
        "# Loop through each engagement field\n",
        "\n",
        "slopes <- c()\n",
        "p_values <- c()\n",
        "intercepts <- c()\n",
        "\n",
        "for (field in engagement_fields) {\n",
        "  # Fit a Poisson fixed effects regression\n",
        "  formula <- as.formula(paste(field, \"~ SEVERE_TOXICITY + 1 + (1|canal)\"))\n",
        "  model <- glmer(formula, family = poisson, data = df_agg)\n",
        "\n",
        "  # Print the model summary\n",
        "\n",
        "  # Extract the slope and p-value for the SEVERE_TOXICITY coefficient\n",
        "  summary <- summary(model)\n",
        "  slope <- summary$coefficients[2, 1]\n",
        "  p_value <- summary$coefficients[2, 4]\n",
        "\n",
        "  # Store the results\n",
        "  slopes <- c(slopes, slope)\n",
        "  p_values <- c(p_values, p_value)\n",
        "\n",
        "  print(\"====================\")\n",
        "}"
      ],
