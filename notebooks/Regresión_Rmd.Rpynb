      "source": [
        "# prompt: I want to perform a Poisson fixed effects regression. Each engagement field should be the dependent variable. The Xs are the toxicity_fields. Each subject is each different author\n",
        "\n",
        "library(lme4)\n",
        "\n",
        "# Loop through each engagement field\n",
        "\n",
        "for (field in engagement_fields) {\n",
        "  # Fit a Poisson fixed effects regression\n",
        "  formula <- as.formula(paste(field, \"~ SEVERE_TOXICITY + (1|canal)\"))\n",
        "  model <- glmer(formula, family = poisson, data = df_agg)\n",
        "\n",
        "  # Print the model summary\n",
        "  print(summary(model))\n",
        "  print(\"====================\")\n",
        "}"
